<resources>

    <style name="AppTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>
    <style name="AppTheme1" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="MyMaterialTheme" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="colorAccent">@color/fcolorAccent</item>
        <item name="colorPrimary">@color/fcolorPrimary</item>
        <item name="colorPrimaryDark">@color/fcolorPrimaryDark</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="TabLayoutTextStyle">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="GalaxyZooThemeToolbarDarkOverflow" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="android:actionOverflowButtonStyle">@style/Widget.ActionButton.Overflow</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="windowActionBar">false</item>
    </style>

    <style name="Widget.ActionButton.Overflow" parent="@android:style/Widget.Holo.ActionButton.Overflow">
        <item name="android:color">#ffffffff</item>
    </style>

    <style name="ButtonWhiteCapsule" parent="@style/Widget.AppCompat.Button">
        <item name="android:background">@drawable/capsule_white</item>
    </style>

    <style name="AppThemess" parent="@style/Theme.AppCompat.Light.DarkActionBar">
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

</resources>
