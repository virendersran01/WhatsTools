<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:n1="http://schemas.android.com/apk/res/android"
    n1:layout_width="fill_parent"
    n1:layout_height="fill_parent">

    <RelativeLayout
        n1:id="@+id/top"
        n1:layout_width="fill_parent"
        n1:layout_height="fill_parent"
        n1:layout_above="@+id/last"
        n1:gravity="center">

        <ProgressBar
            n1:id="@+id/progressBar"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:layout_gravity="center"
            n1:background="@drawable/progres" />

        <WebView
            n1:id="@+id/webViewscan"
            n1:layout_width="fill_parent"
            n1:layout_height="fill_parent" />
    </RelativeLayout>

    <FrameLayout
        n1:layout_marginTop="2dp"
        n1:id="@+id/last"
        n1:layout_width="fill_parent"
        n1:layout_height="wrap_content"
        n1:layout_alignParentBottom="true">

        <com.startapp.android.publish.ads.banner.Banner
            n1:id="@+id/startAppBanner"
            n1:layout_width="wrap_content"
            n1:layout_height="wrap_content"
            n1:layout_gravity="center"/>
    </FrameLayout>

</RelativeLayout>